-- GUI Kick Draggable (by ChatGPT)
local player = game.Players.LocalPlayer
local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local NameLabel = Instance.new("TextLabel")
local KickButton = Instance.new("TextButton")
local UICorner = Instance.new("UICorner")

-- Parent GUI
ScreenGui.Parent = game.CoreGui
ScreenGui.Name = "KickGui"

-- Frame utama
Frame.Size = UDim2.new(0, 230, 0, 90)
Frame.Position = UDim2.new(0.5, -115, 0.75, -45)
Frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
Frame.BackgroundTransparency = 0.3
Frame.BorderSizePixel = 0
Frame.Parent = ScreenGui
Frame.AnchorPoint = Vector2.new(0.5, 0.5)

-- Biar bisa digeser
Frame.Active = true
Frame.Draggable = true

-- Nama di tengah atas
NameLabel.Size = UDim2.new(1, 0, 0.4, 0)
NameLabel.Position = UDim2.new(0, 0, 0, 5)
NameLabel.BackgroundTransparency = 1
NameLabel.Text = "Dhii"
NameLabel.TextColor3 = Color3.fromRGB(0, 255, 255)
NameLabel.TextScaled = true
NameLabel.Font = Enum.Font.SourceSansBold
NameLabel.Parent = Frame

-- Tombol Kick
KickButton.Size = UDim2.new(0.8, 0, 0.4, 0)
KickButton.Position = UDim2.new(0.1, 0, 0.5, 0)
KickButton.BackgroundColor3 = Color3.fromRGB(0, 162, 255)
KickButton.Text = "Keluar (Kick)"
KickButton.TextColor3 = Color3.fromRGB(255, 255, 255)
KickButton.TextScaled = true
KickButton.Font = Enum.Font.SourceSansBold
KickButton.Parent = Frame
KickButton.AutoButtonColor = true

-- Sudut membulat
UICorner.CornerRadius = UDim.new(0, 10)
UICorner.Parent = KickButton

-- Fungsi tombol kick
KickButton.MouseButton1Click:Connect(function()
	player:Kick("woi peju lu udh di kick dhii ganteng sigma tampan.")
end)
